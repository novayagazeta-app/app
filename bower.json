{
    "name": "HelloIonic",
    "private": "true",
    "devDependencies": {
        "ionic": "driftyco/ionic-bower#1.1.0",
        "angular-mocks": "~1.3"
    },
    "dependencies": {
        "underscore": "~1.8.3",
        "linkifyjs": "v2.0.0-beta.1",
        "angulartics": "~0.20.0",
        "ngCordova": "~0.1.20-alpha",
        "swipebox": "https://github.com/brutaldesign/swipebox.git#~1.4.1",
        "angular-swipebox": "git@github.com:novayagazeta-app/angular-swipebox.git",
        "raven-js": "~1.1.19"
    },
    "preen": {
        "SHA-1": ["*.js"],
        "angular": ["*.min.js", "*.map"],
        "angular-animate": ["*.js", "*.map"],
        "angular-mocks": ["*.js"],
        "angular-sanitize": ["*.js", "*.map"],
        "angular-swipebox": ["dist/*.min.js"],
        "angular-ui-router": ["release/*.js"],
        "angulartics": ["dist/*"],
        "ionic": ["css/*", "fonts/*", "js/*"],
        "jquery": ["dist/*"],
        "ngCordova": ["dist/*.js"],
        "raven-js": ["dist/*"],
        "swipebox": ["src/**/*"],
        "underscore": ["*.js", "*.map"],
        "waypoints": ["lib/**/*"]
    }
}
